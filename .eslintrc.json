{
  "extends": ["next/core-web-vitals", "prettier", "next/typescript"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": [
      "error",
      {
        "allow": ["error", "warn"]
      }
    ]
  }
}
